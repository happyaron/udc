#: chapter8/xinclude.xml:3331(para)
msgid "gxine"
msgstr "gxine"

#: chapter8/xinclude.xml:3335(para)
msgid "libdvdcss2"
msgstr "libdvdcss2"

#: chapter8/xinclude.xml:3339(para)
msgid "libdvdnav4"
